{
    "manifest_version": 2,
    "name": "Editify",
    "version": "1.0",
  
    "description": "Makes content on the website editable",
  
    "icons": {
      "48": "icons/editify-48.png",
      "96": "icons/editify-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["editify.js"]
      }
    ]
  
  }